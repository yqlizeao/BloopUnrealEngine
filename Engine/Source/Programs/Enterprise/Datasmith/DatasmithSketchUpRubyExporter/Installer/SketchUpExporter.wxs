						<Directory Id="SketchUp2020UserSketchUpFolder" Name="SketchUp">
							<Directory Id="SketchUp2020UserPlugins" Name="Plugins">
								<Directory Id="SketchUp2020UserPlugins_UnrealDatasmithSketchUp" Name="UnrealDatasmithSketchUp">
									<Directory Id="SketchUp2020UserPlugins_UnrealDatasmithSketchUp_Resources" Name="Resources">
										<Directory Id="SketchUp2020UserPlugins_UnrealDatasmithSketchUp_Resources_de" Name="de">
										</Directory>
