	<Fragment>

		<!-- Declaration of a empty binaries folder so that the engine accept our remote location for the engine dir -->
		<Component Id="EMPTYENGINEBINARIESDIR" Directory="BINARIESFOLDER" Guid="{647E713D-A8C5-4A2A-AA8A-A3244B49EEC5}">
			<CreateFolder/>
		</Component>

		<Component Id="EXPORTER2019" Directory="SketchUp2019UserPlugins" Guid="{AE9233C2-906B-4AC1-83CF-EE439B6AE460}">
			<RegistryKey Root="HKCU" Key="Software\Epic Games\SketchUpPlugin\2019">
				<RegistryValue Name="Installed" Value="1" KeyPath="yes" Type="integer" />
			</RegistryKey>
			<File Id="EXPORTER2019_UnrealDatasmithSketchUp.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp.rb"/>
		</Component>

		<Component Id="EXPORTER2019CODE" Directory="SketchUp2019UserPlugins_UnrealDatasmithSketchUp" Guid="{EBC7AE6B-DEBC-4A65-9D18-0BB8C0D84CAB}">
			<File Id="EXPORTER2019CODE_connections_dialog.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\connections_dialog.rb"/>
			<File Id="EXPORTER2019CODE_DatasmithSketchUp.so" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\DatasmithSketchUp.so"/>
			<File Id="EXPORTER2019CODE_logging.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\logging.rb"/>
			<File Id="EXPORTER2019CODE_messages_dialog.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\messages_dialog.rb"/>
			<File Id="EXPORTER2019CODE_plugin_main.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\plugin_main.rb"/>
			<File Id="EXPORTER2019CODE_utils.rb" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\utils.rb"/>
			<File Id="EXPORTER2019CODE_version" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\version"/>
		</Component>

		<Component Id="EXPORTER2019RESOURCES" Directory="SketchUp2019UserPlugins_UnrealDatasmithSketchUp_Resources" Guid="{D48E9D6F-F777-45B3-9843-999BC6C8FA7D}">
			<File Id="EXPORTER2019RESOURCES_DatasmithAutoSyncIcon16.png" Source="$(var.BinaryDir)\2019\Plugin\UnrealDatasmithSketchUp\Resources\DatasmithAutoSyncIcon16.png"/>
